<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd"
             version="2.2">
    <persistence-unit name="stockmanagement">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>java:comp/env/jdbc/jpa</jta-data-source>
        <class>be.atc.entities.AddressEntity</class>
        <class>be.atc.entities.AdressUsersEntity</class>
        <class>be.atc.entities.BatchsEntity</class>
        <class>be.atc.entities.BrandsEntity</class>
        <class>be.atc.entities.CategoriesEntity</class>
        <class>be.atc.entities.CitiesEntity</class>
        <class>be.atc.entities.CommandsuppliersEntity</class>
        <class>be.atc.entities.CommandsuppliersBatchsEntity</class>
        <class>be.atc.entities.CountriesEntity</class>
        <class>be.atc.entities.DocumentsEntity</class>
        <class>be.atc.entities.DocumenttypesEntity</class>
        <class>be.atc.entities.OrdersEntity</class>
        <class>be.atc.entities.OrdersDocumentsEntity</class>
        <class>be.atc.entities.OrdersProductsEntity</class>
        <class>be.atc.entities.ProductsEntity</class>
        <class>be.atc.entities.ProductsCategoriesEntity</class>
        <class>be.atc.entities.RolesEntity</class>
        <class>be.atc.entities.SuppliersEntity</class>
        <class>be.atc.entities.UsersEntity</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/stockmanagement"/>
            <property name="eclipselink.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
        </properties>

    </persistence-unit>
</persistence>
